<!doctype html>

<head>
    <title>Memes</title>
    <style>
        body {
            background: #333;
            margin: 0;
        }

        img {
            max-width: 100%;
        }

        ul {
            margin: 0;
            padding: 0;
            display: flex;
            flex-direction: column;
            text-align: center;
        }

        li {
            list-style: none;
            padding: 10px;
        }
    </style>
</head>

<body>
    <ul>
    {{#each images}}
        <li>
            <img src="{{this}}" alt="" />
        </li>
    {{/each}}
    </ul>
</body>
